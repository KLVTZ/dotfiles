[user]
	name = Justin Page
	email = justin_page@motortrend.com
[color]
	ui = true
[alias]
	st = status
	co = checkout
	ci = commit
	br = branch
	df = diff
	dfs = diff --staged
	logg = log --graph --decorate --oneline --abbrev-commit --all
[github]
	user = justinpage
[push]
	default = simple
[pull]
	ff = true
[core]
	editor = vim
	excludesfile = /Users/justinpage/.gitignore
[url "git@github.com:"]
	insteadOf = https://github.com/
